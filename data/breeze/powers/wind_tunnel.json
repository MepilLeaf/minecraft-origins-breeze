{
  "type": "origins:multiple",

  "charges": {
    "type": "origins:resource",
    "min": 0,
    "max": 140,
    "start_value": 60,
    "hud_render": {
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_points_01.png",
      "bar_index": 17
    }
  },

  "use_cooldown": {
    "type": "origins:cooldown",
    "cooldown": 6,
    "hud_render": {
      "should_render": false
    }
  },

  "use_buffer_for_recharge": {
    "type": "origins:cooldown",
    "cooldown": 40,
    "hud_render": {
      "should_render": false
    }
  },

  "recharge": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_charges",
      "change": 20
    },
    "interval": 12,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_use_buffer_for_recharge",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "resource": "*:*_charges",
          "comparison": "<=",
          "compare_to": 40
        }
      ]
    }
  },

  "self_up": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "function breeze:wind_tunnel_particles"
        },
        {
          "type": "origins:add_velocity",
          "set": true,
          "y": 1.3
        },
        {
          "type": "origins:play_sound",
          "sound": "minecraft:entity.breeze.shoot",
          "volume": 2.0
        },
        {
          "type": "origins:trigger_cooldown",
          "power": "*:*_use_cooldown"
        },
        {
          "type": "origins:trigger_cooldown",
          "power": "*:*_use_buffer_for_recharge"
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_charges",
          "change": -20
        },
        {
          "type": "origins:trigger_cooldown",
          "power": "*:*_trail_active"
        }
      ]
    },
    "cooldown": 1,
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_charges",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "resource": "*:*_use_cooldown",
          "comparison": "==",
          "compare_to": 0
        }
      ]
    },
    "key": {
      "key": "key.origins.secondary_active"
    }
  },

  "self_directional": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:add_velocity",
          "set": false,
          "space": "local",
          "z": 1.0
        },
        {
          "type": "origins:play_sound",
          "sound": "minecraft:entity.breeze.shoot",
          "volume": 2.0
        },
        {
          "type": "origins:trigger_cooldown",
          "power": "*:*_use_cooldown"
        },
        {
          "type": "origins:trigger_cooldown",
          "power": "*:*_use_buffer_for_recharge"
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_charges",
          "change": -20
        },
        {
          "type": "origins:trigger_cooldown",
          "power": "*:*_trail_active"
        }
      ]
    },
    "cooldown": 1,
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_charges",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "resource": "*:*_use_cooldown",
          "comparison": "==",
          "compare_to": 0
        }
      ]
    },
    "key": {
      "key": "key.origins.primary_active"
    }
  },

  "particles_offset_self_directional": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "execute positioned ~ ~1 ~ run function breeze:wind_tunnel_directional_particles"
    },
    "cooldown": 1,
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_charges",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "resource": "*:*_use_cooldown",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:fall_flying",
          "inverted": true
        },
        {
          "type": "origins:swimming",
          "inverted": true
        }
      ]
    },
    "key": {
      "key": "key.origins.primary_active"
    }
  },

  "no_offset_self_directional": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "execute positioned ~ ~ ~ run function breeze:wind_tunnel_directional_particles"
    },
    "cooldown": 1,
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_charges",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "resource": "*:*_use_cooldown",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:fall_flying"
            },
            {
              "type": "origins:swimming"
            }
          ]
        }
      ]
    },
    "key": {
      "key": "key.origins.primary_active"
    }
  },

  "trail_active": {
    "type": "origins:cooldown",
    "cooldown": 14,
    "hud_render": {
      "should_render": false
    }
  },

  "trail": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "execute positioned ~ ~-1 ~ run function breeze:trail_particles"
    },
    "interval": 1,
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_trail_active",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "name": "§b§nWind Tunnel",
  "description": "§fUsing the air around you, you can use it to propel yourself in a variety of directions. Press §d[Primary Active]§f to launch yourself in the direction you are facing, the speed at which you are launched is based on your current momentum. Press §d[Secondary Active]§f to launch yourself upwards regardless of your current momentum. These abilities consume charges that will replenish shortly after use.",
  "badges": [
    {
      "sprite": "origins:textures/gui/badge/active.png",
      "text": "§d[Primary Active] [Secondary Active]"
    }
  ]
}
