{
  "type": "origins:multiple",

  "charges": {
    "type": "origins:resource",
    "min": 0,
    "max": 140,
    "start_value": 140,
    "hud_render": {
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_points_01.png",
      "bar_index": 17
    }
  },

  "use_cooldown": {
    "type": "origins:cooldown",
    "cooldown": 6,
    "hud_render": {
      "should_render": false
    }
  },

  "use_buffer_for_recharge": {
    "type": "origins:cooldown",
    "cooldown": 60,
    "hud_render": {
      "should_render": false
    }
  },

  "recharge": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_charges",
      "change": 20
    },
    "interval": 3,
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_use_buffer_for_recharge",
      "comparison": "==",
      "compare_to": 0
    }
  },

  "raycast_up": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:raycast",
      "distance": 20.0,
      "block": true,
      "entity": true,
      "shape_type": "outline",
      "fluid_handling": "none",
      "space": "world",
      "bientity_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:target_action",
            "action": {
              "type": "origins:and",
              "actions": [
                {
                  "type": "origins:execute_command",
                  "command": "function breeze:wind_tunnel_particles"
                },
                {
                  "type": "origins:add_velocity",
                  "set": true,
                  "y": 1.8
                },
                {
                  "type": "origins:play_sound",
                  "sound": "minecraft:entity.breeze.shoot",
                  "volume": 2.0
                }
              ]
            }
          }
        ]
      },
      "before_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:trigger_cooldown",
            "power": "*:*_use_cooldown"
          },
          {
            "type": "origins:trigger_cooldown",
            "power": "*:*_use_buffer_for_recharge"
          },
          {
            "type": "origins:play_sound",
            "sound": "minecraft:entity.breeze.jump",
            "volume": 1.0
          },
          {
            "type": "origins:change_resource",
            "resource": "*:*_charges",
            "change": -20
          }
        ]
      },
      "command_along_ray": "particle minecraft:end_rod",
      "command_step": 1,
      "command_along_ray_only_on_hit": false
    },
    "cooldown": 1,
    "hud_render": {
      "should_render": false
    },
    "key": {
      "key": "key.origins.primary_active",
      "continuous": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_charges",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "resource": "*:*_use_cooldown",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:sneaking",
          "inverted": true
        }
      ]
    }
  },

